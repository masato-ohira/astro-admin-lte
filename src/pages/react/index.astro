---
import Layout from '@/layouts/Layout.astro'
import { MyApp } from '@/components/views/MyApp'
import PhpInc from '@/components/PhpInc.astro'
const meta = {
  title: 'React App',
  id: 'index',
}
const isDev = process.env.NODE_ENV == 'development'
---

<Layout meta={meta}>
  {isDev ? <MyApp client:only /> : <PhpInc name={'chakra-app'} />}
</Layout>
